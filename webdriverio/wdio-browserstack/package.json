{
  "name": "wdio-mocha-browserstack-uiautomation",
  "version": "1.0.0",
  "description": "UI Automation using WebdriverIO,Mocha and Browserstack",
  "main": "wdio.conf.js",
  "scripts": {
    "wdio": "wdio wdio.conf.js",
    "report": "allure generate test-output --clean -o reports && allure open reports",
    "bs_local": "./bin/BrowserStackLocal -k 2uWxsmizVqPjGSfEPLw4 -F -forcelocal --local-identifier ragu_local_machine",
    "pretest": "rm -rf test-output",
    "test": "npm run wdio",
    "posttest": "npm run report"
  },
  "keywords": [
    "Selenium",
    "WebdriverIO",
    "Mocha",
    "Chai",
    "Browserstack"
  ],
  "author": "ragupathi ravi",
  "license": "ISC",
  "devDependencies": {
    "allure-commandline": "^2.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "wdio-allure-reporter": "^0.1.2",
    "wdio-browserstack-service": "^0.1.12",
    "wdio-mocha-framework": "^0.5.11",
    "wdio-spec-reporter": "^0.1.2",
    "webdriverio": "^4.8.0"
  }
